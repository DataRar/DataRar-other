<IfModule mod_security.c>
SecFilterEngine Off
SecFilterScanPOST Off
</IfModule>


RewriteEngine On
RewriteCond %{HTTP_HOST} ^www\.hizmet\.datarar\.com$ [NC,OR]
RewriteCond %{HTTP_HOST} ^hizmet\.datarar\.com$ [NC]
RewriteCond %{HTTP_HOST} ^datarar\.com\.hizmet$ [NC,OR]
RewriteRule ^(.*)$ http://datarar.com/hizmetler/$1 [L,R=301]


# Error Pages
ErrorDocument 400 http://datarar.com/hizmetler
ErrorDocument 401 http://datarar.com/hizmetler
ErrorDocument 402 http://datarar.com/hizmetler
ErrorDocument 403 http://datarar.com/hizmetler
ErrorDocument 404 http://datarar.com/hizmetler
